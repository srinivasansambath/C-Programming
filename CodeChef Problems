//The Cheaper Cab Problem
#include <stdio.h>

int main(void) {
    int t;
    int i;
    int a,b;
	scanf("%d",t);
	for(i=0;i<t;i=i+1)
	{
	    scanf("%d %d",&a,&b);
	    if(a<b)
	    {
	        printf("FIRST");
	    }
	    else if(b<a)
	    {
	        printf("SECOND");
	    }
	    else
	    {
	        printf("ANY");
	    }
	}
	return 0;
}

OUTPUT:
3
30 65
42 42
90 50

//Fever Problem
#include <stdio.h>

int main(void) {
	int te,i,t;
	scanf("%d",&te);
	    for(i=0;i<te;i=i+1)
	    {
	        scanf("%d",&t);
	        if(t>98)
	        {
	            printf("YES\n");
	        }
	        else
	        {
	            printf("NO\n");
	        }
	}
	return 0;
}
OUTPUT:
3
98
100
96

// Age Limit problem
#include <stdio.h>

int main(void) {
	int testCases,i,minAge,maxAge,age;
	scanf("%d",&testCases);
	for(i=1;i<=testCases;i=i+1)
	{
	    scanf("%d %d %d",&minAge,&maxAge,&age);
	    if(age>=minAge && age<maxAge)
	    {
	        printf("YES");
	    }
	    else
	    {
	        printf("NO");
	    }
	    printf("\n");
	}
	return 0;
}

OUTPUT:
2
31 42 35
28 30 32
YES
NO

//Tasty Decisions CodeChef Problem
#include <stdio.h>

int main(void) {
	int testCase,i,a,b;
	scanf("%d",&testCase);
	for(i=0; i<testCase; i++)
	{
	   scanf("%d  %d",&a,&b);
	   a = 2*a;
	   b = 5*b;
	   if(a>b)
	   {
	       printf("Chocolate");
	   }
	   else if (b>a)
	   {
	       printf("Candy");
	   }
	   else{
	       printf("Either");
	   }
	   
	   a = 0; b = 0;
	   printf("\n");
	}
	return 0;
}

OUTPUT:
4
5 1
5 2
5 3
3 10

//Chocolate Equal Distribution Program
#include <stdio.h>

int main(void) {
	int testCases,i,a,b;
	scanf("%d",&testCases);
	for(i=0; i<testCases; i++)
	{
	    scanf("%d%d",&a,&b);
	    if((a+b)%2 == 0)
	    {
	        printf("YES");
	    }
	    else
	    {
	        printf("NO");
	    }
	    printf("\n");
	}
	return 0;
}

OUTPUT:
4
1 1
1 3
1 2
1 4

//Is the Score Consistent Problem
#include<stdio.h>

int main() {
	int testCases,a,b,c,d,i;
	scanf("%d",&testCases);
	for(i=0 ; i< testCases ; i++)
	{
	    scanf("%d%d",&a,&b);
	    scanf("%d%d",&c,&d);
	    
	    if((a<=c) && (b<=d))
	    {
	        printf("POSSIBLE");
	    }
	    else
	    {
	        printf("IMPOSSIBLE");
	    }
	    printf("\n");
	}
	return 0;
}
OUTPUT:
3
1 5
3 5
3 4
2 6
2 2
2 2

POSSIBLE
IMPOSSIBLE
POSSIBLE
// rcb and playoff problems
#include <stdio.h>

int main(void) {
        // your code goes here
        int t,a,x,y,z;
        scanf("%d",&t);
        for(int i=0;i<t;i++){
            scanf("%d %d %d",&x,&y,&z);
            a= x+z*2;
            if(a>=y)
            {
            printf("yes\n");
            }
            else
            printf("no\n");
        }
        return 0;
}
//Hardest problem bet - Codechef
#include <stdio.h>
int main(void) 
{
int T,A,B,C;
scanf("%d",&T);
for(int i=1;i<=T;i++)
{
scanf("%d%d%d",&A,&B,&C);
if(A<B && A<C)
printf("Draw\n");
else if(B<A && B<C)
printf("Bob\n");
else
printf("Alice\n");
}
return 0;
}

// BatteryLow problem in CodeChef
#include <stdio.h>

int main() {
	int testCases,i,bt;
	scanf("%d",&testCases);
	for(i=0; i<testCases; i++)
	{
	    scanf("%d",&bt);
	    if(bt <= 15)
	    {
	        printf("Yes");
	    }
	    else
	    {
	        printf("No");
	    }
	}
	return 0;
}
/* OUTPUT
2
10
Yes
20
No
*/







