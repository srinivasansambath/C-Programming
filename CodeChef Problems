//The Cheaper Cab Problem
#include <stdio.h>

int main(void) {
    int t;
    int i;
    int a,b;
	scanf("%d",t);
	for(i=0;i<t;i=i+1)
	{
	    scanf("%d %d",&a,&b);
	    if(a<b)
	    {
	        printf("FIRST");
	    }
	    else if(b<a)
	    {
	        printf("SECOND");
	    }
	    else
	    {
	        printf("ANY");
	    }
	}
	return 0;
}

OUTPUT:
3
30 65
42 42
90 50

//Fever Problem
#include <stdio.h>

int main(void) {
	int te,i,t;
	scanf("%d",&te);
	    for(i=0;i<te;i=i+1)
	    {
	        scanf("%d",&t);
	        if(t>98)
	        {
	            printf("YES\n");
	        }
	        else
	        {
	            printf("NO\n");
	        }
	}
	return 0;
}
OUTPUT:
3
98
100
96

// Age Limit problem
#include <stdio.h>

int main(void) {
	int testCases,i,minAge,maxAge,age;
	scanf("%d",&testCases);
	for(i=1;i<=testCases;i=i+1)
	{
	    scanf("%d %d %d",&minAge,&maxAge,&age);
	    if(age>=minAge && age<maxAge)
	    {
	        printf("YES");
	    }
	    else
	    {
	        printf("NO");
	    }
	    printf("\n");
	}
	return 0;
}

OUTPUT:
2
31 42 35
28 30 32
YES
NO

//Tasty Decisions CodeChef Problem
#include <stdio.h>

int main(void) {
	int testCase,i,a,b;
	scanf("%d",&testCase);
	for(i=0; i<testCase; i++)
	{
	   scanf("%d  %d",&a,&b);
	   a = 2*a;
	   b = 5*b;
	   if(a>b)
	   {
	       printf("Chocolate");
	   }
	   else if (b>a)
	   {
	       printf("Candy");
	   }
	   else{
	       printf("Either");
	   }
	   
	   a = 0; b = 0;
	   printf("\n");
	}
	return 0;
}

OUTPUT:
4
5 1
5 2
5 3
3 10

//Chocolate Equal Distribution Program
#include <stdio.h>

int main(void) {
	int testCases,i,a,b;
	scanf("%d",&testCases);
	for(i=0; i<testCases; i++)
	{
	    scanf("%d%d",&a,&b);
	    if((a+b)%2 == 0)
	    {
	        printf("YES");
	    }
	    else
	    {
	        printf("NO");
	    }
	    printf("\n");
	}
	return 0;
}

OUTPUT:
4
1 1
1 3
1 2
1 4



