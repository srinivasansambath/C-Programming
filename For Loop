// for loop concepts
#include <stdio.h>

int main()
{
    for(;;)
    {
    printf("For loop should have 2 semicolons");
    printf("If no condition is initialized, defaultly it takes as true");
    }
}
// for loop concept
#include <stdio.h>

int main()
{
    int i=1;
    for(i=1;i<=5;i=i+1)
    {
    printf("%d",i);
    }
    printf("\n %d",i);
}
output:
12345
 6

